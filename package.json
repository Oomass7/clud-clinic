{
  "name": "crudclinic",
  "version": "1.0.0",
  "description": "Medical Appointment Management System - Complete Solution",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "db:setup": "cd backend && npm run db:setup",
    "db:seed": "cd backend && npm run db:seed",
    "test:connection": "cd backend && npm run test:connection"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["medical", "appointments", "crud", "express", "postgresql", "vite"],
  "author": "Mariana Carmona",
  "license": "MIT"
}
